{
  "name": "react-faceted-token-input",
  "version": "1.0.2",
  "description": "React Faceted Token Input",
  "main": "./lib/index.js",
  "jsnext:main": "./src/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src --out-dir lib",
    "clean": "rimraf lib",
		"flow": "flow",
    "lint": "eslint src test",
    "test": "cross-env NODE_ENV=test jest",
    "start": "cross-env NODE_ENV=development node ./server",
    "prepublish": "npm run build && npm run lint"
  },
  "files": [
    "lib",
    "src"
  ],
  "author": "Pier Paolo Ramon <pierpaolo.ramon@smc.it> (https://github.com/yuchi)",
  "license": "LGPL-2.1",
  "dependencies": {
    "classnames": "^2.2.2"
  },
  "peerDependencies": {
    "react": "^0.14.5"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.3.26",
    "babel-eslint": "^4.1.6",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "cross-env": "^1.0.7",
    "eslint": "^1.10.3",
    "eslint-config-semistandard": "^5.0.0",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-react": "^3.14.0",
    "eslint-plugin-standard": "^1.3.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "flow-bin": "^0.30.0",
    "jest-cli": "^0.8.2",
    "react": "^0.14.5",
    "react-a11y": "^0.3.1",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.5",
    "rimraf": "^2.5.0",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smclab/react-faceted-token-input.git"
  },
  "keywords": [
    "react",
    "token",
    "input",
    "faceted",
    "component"
  ],
  "bugs": {
    "url": "https://github.com/smclab/react-faceted-token-input/issues"
  },
  "homepage": "https://github.com/smclab/react-faceted-token-input#readme",
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testDirectoryName": "test",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ]
  }
}
